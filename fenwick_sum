struct fenwick
{
    int T[maxn2 + 5];
    int cur[maxn2 + 5];
    int query(int i)
    {
        int ans = 0;
        for(i; i >= 0; i = (i & (i + 1)) - 1)
            ans += T[i];
        return ans;
    }
    void update(int i, ll x)
    {
        int v = i;
        for(i; i < maxn2; i = i | (i + 1))
        {
            T[i] -= cur[v];
            T[i] += x;
        }
        cur[v] = x;
    }
    int query(int l, int r)
    {
        return query(r) - query(l - 1);
    }
};
